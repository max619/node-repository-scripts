    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Cli",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "runtimeExecutable": "yarn",
            "args": ["ts-node", "${workspaceFolder}/src/cli.ts", "scriptEcho", "mixedEcho", "echo", "echoSeries", "--script", "${workspaceFolder}/test_scripts/echoScripts.ts"]
        }
    ]
}